<?xml version="1.0" encoding="utf-8"?>
<forms>
	<form id="fuzz">
		<fieldset class="clearfix">
			<ol>
				<li>
					<text id="text" name="text">
						<label><![CDATA[Text <em class="required">*</em>]]></label>
						<error validation="Boo_Validator::isString(#, 1, 20);">Text is not valid.</error>
						<error required="true" validation="Boo_Validator::isRequired(#);">Text is required.</error>
					</text>
				</li>
				<li>
					<submit id="fuzz-button" name="fuzz-button" class="button button-submit" value="Submit"></submit>
				</li>
			</ol>
		</fieldset>
	</form>
</forms>